{
  "schemaVersion": 1,
  "id": "createdeco",
  "version": "${version}",

  "name": "Create Deco",
  "description": "Create Deco, for when your intricate spinning factory needs a little more beauty. Adds several varieties of decorative blocks to fill out Create's palette.",
  "authors": [
    "Talrey [Code]",
    "Kayladillo [Art]",
    "AlphaMode [Fabric]"
  ],
  "contact": {
    "issues": "https://github.com/talrey/CreateDeco/issues",
    "sources": "https://github.com/talrey/CreateDeco"
  },

  "license": "All rights reserved",
  "icon": "createdeco_ico.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "com.github.talrey.createdeco.CreateDecoMod"
    ],
    "fabric-datagen": [
      "com.github.talrey.createdeco.CreateDecoModData"
    ]
  },

  "accessWidener": "createdeco.accesswidener",

  "depends": {
    "fabricloader": ">=0.13.1",
    "fabric": ">=0.46.0",
    "minecraft": ">=1.18.1",
    "registrate-fabric": "*",
    "flywheel": "*",
    "create": "*"
  }
}